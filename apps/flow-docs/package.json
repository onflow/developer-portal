{
  "name": "flow-docs-site",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "workspaces": {
    "nohoist": [
      "express",
      "morgan",
      "compression",
      "body-parser",
      "@remix-run/**"
    ]
  },
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build",
    "dev": "run-p dev:*",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ../../node_modules/dotenv/config --require ./mocks ./build/server.js",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate:css": "tailwindcss -c ../../tailwind.config.js -i ../../libs/design-system/styles/main.css -o ./app/styles/main.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "typecheck": "tsc -b"
  },
  "prettier": {
    "semi": false
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@kentcdodds/md-temp": "^1.1.0",
    "@mdx-js/esbuild": "^2.1.1",
    "@mdx-js/mdx": "^2.1.1",
    "@octokit/plugin-throttling": "^3.6.2",
    "@octokit/rest": "^18.12.0",
    "@octokit/core": "^3.6.0",
    "@prisma/client": "^3.11.1",
    "@remark-embedder/core": "^2.0.0",
    "@remark-embedder/transformer-oembed": "^2.0.0",
    "@remix-run/express": "^1.4.3",
    "@remix-run/node": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "@remix-run/server-runtime": "^1.4.3",
    "@types/remark-prism": "^1.3.3",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.1.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "express": "^4.17.3",
    "hast-util-to-string": "^2.0.0",
    "ioredis": "^5.0.5",
    "lru-cache": "^7.10.1",
    "mdx-bundler": "^9.0.0",
    "morgan": "^1.10.0",
    "on-finished": "^2.4.1",
    "p-queue": "^7.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "reading-time": "^1.5.0",
    "rehype-document": "^6.0.1",
    "rehype-format": "^4.0.1",
    "rehype-parse": "^8.0.4",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-prism": "^1.3.6",
    "remark-rehype": "^10.1.0",
    "remark-slug": "^7.0.1",
    "tiny-invariant": "^1.2.0",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.1",
    "@reach/dialog": "^0.17.0",
    "@remix-run/dev": "^1.4.3",
    "@remix-run/eslint-config": "^1.4.3",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.0.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.1",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@types/on-finished": "^2.3.1",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.4",
    "c8": "^7.11.0",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.29",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^2.55.0",
    "msw": "^0.39.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "prisma": "^3.11.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.8.2"
  },
  "engines": {
    "node": ">=16"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "resolutions": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
